
@{
    ViewBag.Title = "Publish";
}

<center><h2>Catalog</h2></center>

<div class="row">
    <div class="col-sm-8 col-sm-offset-2">
        <div class="panel panel-primary">
            <div class="panel-heading">
                <h3 class="panel-title">List of Assays in Catalog Bank</h3>
            </div>
            <div class="panel-body">
                <table class="table table-striped table-hover ">
                    <thead>
                        <tr>
                            <th>ID</th>
                            <th>Assay Name</th>
                            <th>Creation Date</th>
                            <th>Last Modified</th>
                            <th>Details</th>
                            <th>Include</th>
                        </tr>
                    </thead>
                    <tbody>
                        <tr>
                            <td>1</td>
                            <td>Biochemical Pharmacology (BP)</td>
                            <td>2016-04-09</td>
                            <td>2016-09-24</td>
                            <td>@Html.ActionLink("View", "Details", "Catalog")</td>
                            <td><input type="checkbox" checked/></td>
                        </tr>
                        <tr>
                            <td>2</td>
                            <td>DiscoveryScreen® (DS)</td>
                            <td>2015-07-10</td>
                            <td>2016-04-06</td>
                            <td>@Html.ActionLink("View", "Details", "Catalog")</td>
                            <td><input type="checkbox" checked/></td>

                        </tr>
                        <tr>
                            <td>3</td>
                            <td>ImmunoScreen® (IS)</td>
                            <td>2014-05-26</td>
                            <td>2015-01-30</td>
                            <td>@Html.ActionLink("View", "Details", "Catalog")</td>
                            <td><input type="checkbox" checked/></td>
                        </tr>
                        <tr>
                            <td>4</td>
                            <td>ProfilingScreen® (PF)</td>
                            <td>2014-09-18</td>
                            <td>2016-02-31</td>
                            <td>@Html.ActionLink("View", "Details", "Catalog")</td>
                            <td><input type="checkbox" /></td>
                        </tr>
                        <tr>
                            <td>5</td>
                            <td>PharmaScreen® (PS)</td>
                            <td>2015-01-30</td>
                            <td>2016-08-29</td>
                            <td>@Html.ActionLink("View", "Details", "Catalog")</td>
                            <td><input type="checkbox" checked/></td>
                        </tr>
                        <tr>
                            <td>6</td>
                            <td>CustomScreen™ (CS)</td>
                            <td>2015-11-30</td>
                            <td>2016-12-01</td>
                            <td>@Html.ActionLink("View", "Details", "Catalog")</td>
                            <td><input type="checkbox" checked/></td>
                        </tr>
                    </tbody>
                </table>
            </div>
        </div>
        <center><button class="btn btn-primary" onclick="publish()">Publish</button></center>
        <div id="success" style="display: none;">
            <p></p>
            <center><div class="alert alert-dismissible alert-success">
                <button type="button" class="close" data-dismiss="alert">&times;</button>
                <strong>Success!</strong> Your changes have been saved and the published content has been updated.
            </div></center>
        </div>
    </div>
</div>

<script>
    function publish() {
        $("#success").show();
    }
</script>